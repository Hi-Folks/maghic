{
    "name": "roberto-butti/maghic",
    "description": "Maghic: MAnaging GitHub .",
    "keywords": ["github actions", "console", "cli"],
    "homepage": "https://hi-folks.dev",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/roberto-butti/maghic/issues",
        "source": "https://github.com/roberto-butti/maghic"
    },
    "authors": [
        {
            "name": "Roberto Butti",
            "email": "roberto.butti@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/framework": "^8.8",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3",
        "phpstan/phpstan": "^1.2",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["maghic"]
}
